<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.rock.mappers.UserInfoMapper">

    <select id="getUserInfo" resultType="com.spring.rock.entity.UserInfoEntity">
        SELECT `id` AS userId,account AS userName FROM rk_user
        limit 1;
    </select>

</mapper>
